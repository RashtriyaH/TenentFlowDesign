<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TenentFlowDesign.View.MyActivity.LastPage"
             Title="LastPage"
             BackgroundColor="{StaticResource Black}">
    <Shell.NavBarIsVisible>False</Shell.NavBarIsVisible>
    <Frame HorizontalOptions="Center" VerticalOptions="Center" BackgroundColor="Transparent" BorderColor="Transparent" >
        <Grid HeightRequest="150" WidthRequest="348" >
            <Image Source="background_image.png" Aspect="Center">
                <Image.GestureRecognizers>
                    <TapGestureRecognizer  Command="{Binding MyActivityPageToMoveCommand}" CommandParameter="2" />
                </Image.GestureRecognizers>
            </Image>
            <Label Text="Coming Soon !" HorizontalTextAlignment="Center" FontSize="25" VerticalOptions="Center" TextColor="#F3E29D" />
        </Grid>
    </Frame>
</ContentPage>